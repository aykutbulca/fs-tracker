{
  "name": "mirror",
  "productName": "Mirror",
  "version": "0.0.1",
  "description": "A cross platform file system tracker application",
  "main": "main.js",
  "scripts": {
    "build": "webpack --config webpack.build.config.js",
    "dev": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js",
    "package": "webpack --config webpack.build.config.js",
    "postpackage": "electron-packager ./ --out=./builds",
    "prod": "webpack --config webpack.build.config.js && electron --noDevServer ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aykutbulca/mirror.git"
  },
  "keywords": [
    "file",
    "system",
    "tracker"
  ],
  "author": "Aykut Bulca <aykutbulca@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aykutbulca/mirror/issues"
  },
  "homepage": "https://github.com/aykutbulca/mirror#readme",
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "electron": "^2.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-electron": "^1.4.15",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^0.28.11",
    "electron-packager": "^12.0.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  }
}
